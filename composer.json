{
    "name": "rootwork/phalcon-devtools",
    "type": "library",
    "description": "Custom build of Phalcon devtools that provides the simple-migration commend.",
    "keywords": ["framework", "phalcon", "devtools", "webtools", "phar"],
    "homepage": "https://github.com/rootworkit/phalcon-devtools",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Phalcon Team",
            "email": "team@phalconphp.com",
            "homepage": "https://phalconphp.com/en/team"
        },
        {
            "name": "Contributors",
            "homepage": "https://github.com/phalcon/phalcon-devtools/graphs/contributors"
        },
        {
            "name": "Mike Soule",
            "email": "mike@rootwork.it"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-phalcon": "^3.0"
    },
    "require-dev": {
        "kherge/box": "^2.7"
    },
    "autoload": {
        "psr-4" : {
            "Phalcon\\" : "scripts/Phalcon/"
        }
    },
    "replace": {
        "phalcon/devtools": "3.0.x"
    },
    "bin": ["phalcon.php"]
}
